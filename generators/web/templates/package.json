    "aot-compile": "ngc -p Angular/aot/tsconfig-aot.json",
    "tree-shake": "rollup -c Angular/aot/rollup-config.js",
    "build": "npm run tsc && npm run sass && npm run copy-to-aot && npm run aot-compile && npm run tree-shake",
    "start:aot": "node ./www --env=production"
  },
